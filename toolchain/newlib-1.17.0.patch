+	unsigned long return_value=0, error_no = 0;
+	
+	asm volatile("	movl %2, %%eax; \
+					movl %3, %%ebx; \
+					movl %4, %%ecx; \
+					movl %5, %%edx; \
+					movl %6, %%esi; \
+					movl %7, %%edi; \
+					int $0x80; 		\
+					mov %%eax, %0; 	\
+					mov %%ebx, %1	\
